{
  "name": "fhevm-examples-generator",
  "version": "1.0.0",
  "description": "Tooling for generating standalone FHEVM example repositories and documentation",
  "scripts": {
    "create": "npx ts-node scripts/create.ts",
    "create-example": "npx ts-node scripts/create.ts example",
    "create-category": "npx ts-node scripts/create.ts category",
    "create-docs": "npx ts-node scripts/create.ts docs",
    "create-docs-all": "npx ts-node scripts/create.ts docs --all",
    "create help": "npx ts-node scripts/create.ts help",
    "test-all": "npx ts-node scripts/maintenance.ts test-all",
    "generate:config": "npx ts-node scripts/generate-config.ts",
    "sync:config": "npx ts-node scripts/sync-config.ts",
    "postinstall": "git submodule update --init --recursive"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "fhevm",
    "zama",
    "encryption",
    "blockchain",
    "examples",
    "documentation",
    "generator"
  ],
  "author": "Zama",
  "license": "BSD-3-Clause-Clear",
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NecipAkgz/fhevm-example-factory"
  },
  "devDependencies": {
    "@clack/prompts": "^0.11.0",
    "@types/node": "^20.19.8",
    "picocolors": "^1.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
